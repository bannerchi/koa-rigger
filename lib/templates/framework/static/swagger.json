{
    "swagger": "2.0",
    "info": {
        "title": "vidaXl Order Service 2.0 API",
        "description": "vidaxl order service api by Koa.js",
        "version": "1.0.0"
    },
    "host": "localhost:3000",
    "schemes": [
        "http"
    ],
    "basePath": "/api/order",
    "produces": [
        "application/json"
    ],
    "paths": {
        "/v2/{code}": {
            "get": {
                "summary": "Get order for custmer portal",
                "description": "get one order for cp2 project\n",
                "parameters": [
                    {
                        "name": "code",
                        "in": "path",
                        "description": "Latitude component of location.",
                        "required": true,
                        "type": "string"
                    }
                ],
                "tags": [
                    "Order"
                ],
                "responses": {
                    "200": {
                        "description": "An object of order",
                        "schema": {
                            "type": "object"
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
        "/_makecrypt": {
            "get": {
                "summary": "make code security",
                "description": "make order_id and sales_office_id encrypt\n",
                "parameters": [
                    {
                        "name": "order_id",
                        "in": "query",
                        "description": "order id is poNumber",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "sales_office_id",
                        "in": "query",
                        "description": "sales_office_id",
                        "required": true,
                        "type": "string"
                    }
                ],
                "tags": [
                    "makecrypt"
                ],
                "responses": {
                    "200": {
                        "description": "An string of code",
                        "schema": {
                            "$ref": "#/definitions/Code"
                        }
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "Code": {
            "type": "string",
            "description": "encrypted string\n"
        },
        "Error": {
            "type": "string"
        }
    }
}